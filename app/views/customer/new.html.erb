
<h1> Rejestracja </h1>


<table class="myTable">
<pre>
<%= form_for @customer, url: {action: "create"} do |f| %>
</pre>
  <tr>
    <th>Login</th>
	<td><%= f.text_field :login %></td>
  </tr>

  <tr>
    <th>Imie</th>
	<td><%= f.text_field :name %> </td>
  </tr>
  <tr>
	<th>Nazwisko</th>
	<td> <%= f.text_field :surname %></td>
  </tr>

  <tr>
	<th>Data urodzenia</th>
	<td> <%= f.date_field :birth_date %></td>
  </tr>
  <tr>
	<th>Email</th>	
	<td><%= f.email_field :email %></td>
  </tr>
  <tr>
	<th>Potwierdz Email</th>	
	<td><%= f.email_field :email_confirmation %></td>
  </tr>
  <tr>
     <th>Hasło</th>	
     <td><%= f.password_field :password %></td>
  </tr>
  <tr>
     <th>Potwierdz Hasło</th>	
     <td><%= f.password_field :password_confirmation %></td>
  </tr>
  <tr>
    <th>Kraj</th>
    <td> <%= f.text_field :country %></td>	
  </tr>
  <tr>
    <th>Miasto</th>
    <td> <%= f.text_field :city %></td>	
  </tr>
  <tr>
    <th>Ulica</th>
    <td> <%= f.text_field :street %></td>	
  </tr>
  <tr>
    <th>Kod pocztowy</th>
    <td> <%= f.text_field :zip_code %></td>	
  </tr>		
</table>
	<%= f.submit "Zarejestruj"%>

<% end %>
